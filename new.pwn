cmd:creategarb(playerid, params[])
{
	if(p_info[playerid][admin] < 8) return 1;
	extract params -> new garbs_type; else return SendClientMessage(playerid, col_gray, !"Используйте: "c_white"/creategarb [0 - большая | 1 - маленькая]");
	
	new Float: posX, Float: posY, Float: posZ, Float: posA;
	GetPlayerPos(playerid, posX, posY, posZ);
	GetPlayerFacingAngle(playerid, posA);

	new r[3] = {1334 , 1331, 3035}, vw = GetPlayerInterior(playerid), int = GetPlayerInterior(playerid);
	if(params[0] == 0)
		garb_info[TOTAL_GARB][g_object] = CreateDynamicObject(r[random(3)], posX , posY , posZ - 0.2 , 0.0 , 0.0 , posA , vw, int);
	else 
		garb_info[TOTAL_GARB][g_object] = CreateDynamicObject(11706 , posX , posY , posZ - 1 , 0.0 , 0.0 , posA , vw, int);

	garb_info[TOTAL_GARB][g_area] = CreateDynamicSphere(posX , posY , posZ , 2.0, vw, int);

	TOTAL_GARB++;

	new query_string[110];
	format(query_string, sizeof(query_string), "INSERT INTO `garbage` (`type`,`pos`,`intvw`) VALUES ('%d','%.3f,%.3f,%.3f,%.3f','%d,%d')", params[0], posX, posY, posZ, posA, vw, int);
	mysql_query(sql_connection, query_string);
	return 1;
}

