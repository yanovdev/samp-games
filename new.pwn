public OnPlayerClickTextDraw(playerid, Text:clickedid)
{
	if(p_t_info[playerid][p_dialog] != -1) return 1;

	if(clickedid == Text:INVALID_TEXT_DRAW)
	{
		if(trns_actives[playerid] >= 0 && trns_actives[trns_actives[playerid]] >= 0)
		{
			return cancel_trns(playerid);
		}

		if(inv_active[playerid]) return hide_player_inventory(playerid);
	}

	if(inv_active[playerid])
	{
		if(trns_active[playerid])
		{
			if(clickedid == TextDrawTransfer[1])// accept
			{
				new players = trns_actives[playerid];

				if(trns_active[players] == 1)// если согласен первый
				{
					SendClientMessage(playerid , col_white , "Вы согласились на передачу предметов. Ожидайте подтверждения другой стороны");
					trns_active[players] = 2;
					return 1;
				}
				else if(trns_active[players] == 2)// если согласен второй
				{
					if(p_info[players][money] < trns_player_m[playerid]) return SendClientMessage(playerid, col_gray,"У игрока нету такой суммы. Выбирите другую");

					if(trns_player_m[playerid] > 999)
					{
						give_money(playerid , - trns_player_m[playerid]);
						give_money(trns_actives[playerid], trns_player_m[playerid]);
					}
					else if(trns_player_m[players] > 999)
					{
						give_money(trns_actives[playerid], - trns_player_m[playerid]);
						give_money(playerid , trns_player_m[playerid]);
					}
					for(new i = 0; i < 5; i++)
					{
						if(trns_player[playerid][i][0] > 0)give_item(players , trns_player[playerid][i][0], trns_player[playerid][i][1]);
						if(trns_player[players][i][0] > 0)give_item(playerid , trns_player[playerid][i][0], trns_player[playerid][i][1]);
					}

					cancel_trns(players);
					cancel_trns(playerid);

					SendClientMessage(playerid , col_white , "Сделка по обмену предметов произошла упешно.");

					return 1;
				}

			}
			if(clickedid == TextDrawTransfer[2])// cancel
			{
				new scm_query[140];
				format(scm_query , sizeof(scm_query), ""c_green"%s"c_white"["c_green"%d"c_white"]отказался от Вашего предложения обмена предметами.", p_info[trns_actives[playerid]][name], trns_actives[playerid]);
				SendClientMessage(trns_actives[playerid], col_white , scm_query);
				SendClientMessage(playerid , col_white , "Вы отказался от предложения обмена предметами.");

				cancel_trns(trns_actives[playerid]);
				cancel_trns(playerid);
				return 1;
			}
		}
		if(GetPVarInt(playerid, !"Inv: Select") > -1)
		{
			new slot = GetPVarInt(playerid, !"Inv: Select");
			if(clickedid == td_inventory[1]) return useInvItem(playerid, p_info[playerid][inv][slot]);
			else if(clickedid == td_inventory[4])
			{
				new query_string[200], item = p_info[playerid][inv][slot];
				format(query_string, sizeof(query_string), \
					""c_server"%s\n\n\
					"c_white"Количество: "c_silver"%d\n\
					"c_white"Общая масса: "c_silver"%.3f кг\n\
					"c_white"Масса предмета: "c_silver"%.3f кг",
					inv_info[item][i_name],
					p_info[playerid][inv_count][slot],
					(item >= 123 && item <= 131) ? (inv_info[item][i_count] + (inv_info[135][i_count] * p_info[playerid][inv_count][slot])) : (inv_info[item][i_count] * p_info[playerid][inv_count][slot]),
					inv_info[item][i_count]);
				return show_dialog(playerid, d_none, DIALOG_STYLE_MSGBOX, !""scm_dialog"Информация о предмете", query_string, !"Закрыть", !"");
			}
			else if(clickedid == td_inventory[5])
			{
				switch(p_info[playerid][inv][slot])
				{

				}
			}
		}
		if(clickedid == td_inventory[3])
		{
			if(p_info[playerid][level] < 2) return SendClientMessage(playerid , col_gray , "Вы не можете передать предметы пока не достигнете 2-го уровня");
			return show_dialog(playerid, d_tnsf, DIALOG_STYLE_INPUT, ""c_server"Передача", ""c_white" Введите ID игрока для передачи ему предметов" , "Перевод", "Закрыть");
		}
		else if(clickedid == td_inventory[2])// drop
		{
			if(add_section[playerid]) return hide_add_section(playerid);
			if(player_garb_area[playerid] >= 0)
			{
				show_add_section(playerid , 0);

				SetPlayerChatBubble(playerid, "Открыл мусорку", col_light_purple, 13, 5000);

				return 1;
			}
			if(player_veh_area[playerid] >= 0)
			{
				if(! v_boot_a(player_veh_area[playerid])) return SendClientMessage(playerid, col_gray, "В машине закрыт багажник. Откройте его для загрузки предметов");
				show_add_section(playerid , 1);

				SetPlayerChatBubble(playerid, "Заглянул в багажник рядом стоящей машины", col_light_purple, 13, 5000);

				return 1;
			}
		}
	}

	if(reg_info[playerid][reg_national] != 0)
	{
		if(!(_:clickedid ^ 0xFFFF)) return SelectTextDraw(playerid, 0xB0C4DEFF);
	}
	if(GetPVarInt(playerid, "tshop_type") > 0)
	{
		if(!(_:clickedid ^ 0xFFFF))
		{
			SelectTextDraw(playerid, 0xB0C4DEFF);
			return 1;
		}
	}
	if(GetPVarInt(playerid, "p_skin_shop") == 1 || GetPVarInt(playerid, "p_sm_used"))
	{
		if(!(_:clickedid ^ 0xFFFF))
		{
			SelectTextDraw(playerid, 0xB0C4DEFF);
			return 1;
		}
