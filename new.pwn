cmd:heal(playerid, params[])
{
	if(!medic_player(playerid)) return 1;
	extract params -> new player:targetid, price_heal; else return SendClientMessage(playerid, col_gray, !"Используйте: /heal [id игрока] [стоимость]");
	if(!IsPlayerConnected(targetid) || targetid == playerid) return SendClientMessage(playerid, col_white, !""scm_error"Игрок не найден.");
    if(!IsPlayerInRangeOfPoint(playerid, 5.0, p_t_info[targetid][p_pos][0], p_t_info[targetid][p_pos][1], p_t_info[targetid][p_pos][2]) || GetPlayerVirtualWorld(targetid) != GetPlayerVirtualWorld(playerid)) return SendClientMessage(playerid, col_white, !""scm_error"Игрок слишком далеко.");
	if(price_heal < 1 || price_heal > 1000) return SendClientMessage(playerid, col_white, !""scm_error"Стоимость лечения не может быть меньше 1$ или больше 1.000$.");
	if(p_t_info[targetid][p_dialog] != -1) return SendClientMessage(playerid, col_white, !""scm_error"У игрока открыт диалог.");

	new Float:pl_health;
	GetPlayerHealth(playerid, pl_health);
	if(pl_health >= 99) return SendClientMessage(playerid, col_white, !""scm_error"Данный игрок не нуждается в лечении.");

	new dialog_string[118];
	format(dialog_string, sizeof(dialog_string), ""scm_info"Вы предложили "c_server"%s"c_white" лечение за "c_green"%d$", p_info[targetid][name], price_heal);
	SendClientMessage(playerid, col_white, dialog_string);

	format(dialog_string, sizeof(dialog_string), "Сотрудник "c_server"%s"c_white" предлагает Вам вылечиться за "c_green"%d$", p_info[playerid][name], price_heal);
	show_dialog(targetid, d_heal, DIALOG_STYLE_MSGBOX, !""scm_dialog"Медицинское обслуживание", dialog_string, !"Далее", !"Закрыть");

	heal_price[targetid] = price_heal;
	heal_id[targetid] = playerid;
	update_activity(playerid);
	return 1;
}

