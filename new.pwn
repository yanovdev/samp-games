callback: check_player_banned(playerid)
{
	new rows;
	cache_get_row_count(rows);

	if(rows)
	{
		global_string[0] = EOS;
		new dates , reason[64], admin_name[MAX_PLAYER_NAME], ub_days , old_date;
		cache_get_value_name(0, "u_b_admin", admin_name, MAX_PLAYER_NAME);
		cache_get_value_name(0, "u_b_reason", reason, 64);
		cache_get_value_name_int(0, "u_b_date", dates);
		cache_get_value_name_int(0, "u_b_ndate", old_date);
		cache_get_value_name_int(0, "u_b_days", ub_days);

		format(global_string, sizeof global_string, ""c_red"Ваш аккаунт был заблокирован, за нарушение правил сервера.\n\n\
	    "c_grey" * Пожалуйста, сделайте скриншот данного окна "c_white"(F8),\n\
        "c_grey"   если вы не согласны с наказанием и оставьте жалобу на форуме "c_white""SERVER_URL".\n\n\
		"c_server""c_grey"Ваш ник: "c_white"%s.\n\
	    "c_server""c_grey"Ник администратора: "c_white"%s.\n\
	    "c_server""c_grey"Дата блокировки: "c_white"%s.\n\
	    "c_server""c_grey"Дата разблокировки: "c_white"%s(%d дней)\n\
	    "c_server""c_grey"Причина: "c_white"%s\n",
		p_info[playerid][name], admin_name, date("%dd.%mm.%yyyy %hh:%ii", dates), date("%dd.%mm.%yyyy %hh:%ii", old_date), ub_days, reason);
		show_dialog(playerid, d_none, DIALOG_STYLE_MSGBOX,""c_server"Информация о блокировке", global_string, "Выйти", "");
		kick_player(playerid);
	}
	else
	{
		show_login(playerid);
		SetPVarInt(playerid, "login_timer", _:SetTimerEx("login_timer", 60000, false, "i", playerid));
	}
	return 1;
}


