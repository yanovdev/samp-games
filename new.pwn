				format(_query, 72, "~n~~n~~n~~n~~g~+1 ~w~SKILLS RIFLE ~n~ALL: ~g~ %i/100", p_info[playerid][gun_skills][6]);
				GameTextForPlayer(playerid, _query, 2000, 5);
