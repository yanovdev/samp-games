			foreach(new veh_id: player_vehicles[playerid])
			{
				new line_string[70];
				format(line_string, sizeof(line_string), ""c_white"%s — "c_silver"%s\n", vehicle_name[GetVehicleModel(veh_id)- 400], veh_info[veh_id - 1][v_plate]);
				strcat(dlobal_string, line_string);
			}
			show_dialog(playerid, d_delivery_car_island_1, DIALOG_STYLE_TABLIST, !""scm_dialog"Доставка транспорта", dlobal_string, !"Выбрать", !"Закрыть");
		}
		case d_delivery_car_island_1:
		{
			if(!response) return 1;
			if(p_info[playerid][money] < 5000) return SendClientMessage(playerid, col_white, !""scm_error"У Вас недостаточно средств.");

			new _step = 0, carid;
			foreach(new _v_id: player_vehicles[playerid])
			{
				if(_step == listitem)
				{
					carid = _v_id;
				}
				_step++;
			}
			foreach(new i: logged_players)
			{
				if(IsPlayerInVehicle(i, carid)) return SendClientMessage(playerid, col_white, !""scm_error"Транспорт используется.");
			}
			if(!IsPlayerInRangeOfPoint(playerid, 50.0, veh_info[carid - 1][v_now_pos][0], veh_info[carid - 1][v_now_pos][1], veh_info[carid - 1][v_now_pos][2]) && veh_info[carid - 1][v_vw] == GetPlayerVirtualWorld(playerid)) return SendClientMessage(playerid, col_white, !""scm_error"Транспорт слишком далеко.");

			SetPVarInt(playerid, !"delivery_vehicle", carid);
			show_dialog(playerid, d_delivery_car_island_2, DIALOG_STYLE_LIST, !""scm_dialog"Доставка транспорта", !""c_server"1 | "c_white"Los Santos\n"c_server"2 | "c_white"San Fierro\n"c_server"3 | "c_white"Las Venturas", !"Выбрать", !"Назад");
		}
		case d_delivery_car_island_2:
		{
			if(!response) return 1;
			if(p_info[playerid][money] < 5000) return SendClientMessage(playerid, col_white, !""scm_error"У Вас недостаточно средств.");

			give_money(playerid, -5000);
			insert_money_log(playerid, INVALID_PLAYER_ID, -5000, "доставка семейного транспорт на остров");

			new random_car_pos = random(12), carid = GetPVarInt(playerid, !"delivery_vehicle");
			veh_info[carid - 1][v_pos][0] = delivery_car_pos_pier[listitem][random_car_pos][0];
			veh_info[carid - 1][v_pos][1] = delivery_car_pos_pier[listitem][random_car_pos][1];
			veh_info[carid - 1][v_pos][2] = delivery_car_pos_pier[listitem][random_car_pos][2];
			veh_info[carid - 1][v_pos][3] = delivery_car_pos_pier[listitem][random_car_pos][3];

			SetVehiclePos(carid, veh_info[carid - 1][v_pos][0], veh_info[carid - 1][v_pos][1], veh_info[carid - 1][v_pos][2]);
			SetVehicleZAngle(carid, veh_info[carid - 1][v_pos][3]);
		}
	}
	return 1;
}

