public OnPlayerCommandReceived(playerid, cmd[], params[], flags)
{
 	#if defined debug_mode
		printf("[DEBUG][OnPlayerCommandReceived]playerid: %d | cmd:%s", playerid, cmd);
	#endif
	if(p_t_info[playerid][p_logged] == false) return SendClientMessage(playerid, col_gray,"Вы не авторизованы."), false;
 	if(p_t_info[playerid][p_dialog] != -1) return SendClientMessage(playerid, col_gray,"У Вас открыт диалог, Вы не можете использовать команды."), false;

	if(vk_checking { playerid } == 1 || google_checking { playerid } == 1) return SendClientMessage(playerid, col_gray,"Вы не авторизовались."), false;

 	if(p_info[playerid][acces_edit]) return SendClientMessage(playerid, col_gray,"У Вас открыт редактор, Вы не можете использовать команды."), false;

	if(GetTickCount()- cmd_cooldown[playerid] < 1000)
	{
	    if(CountFloodForPlayer[playerid] >= 2) return 0;
		CountFloodForPlayer[playerid]++;
		SendClientMessage(playerid, col_gray,"Не флудите.");
		return 0;
	}
	cmd_cooldown[playerid] = GetTickCount();

	return 1;
}
