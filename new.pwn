CMD:demote(playerid, params[])
{
	if(! cop_player(playerid)&& ! fbi_player(playerid)) return 1;
	if(p_info[playerid][rank] < 6) return 1;
	if(sscanf(params, "u", params[0])) return SendClientMessage(playerid, col_gray,"Используйте: /frisk[ид/имя]");
	if(! IsPlayerConnected(params[0])) return SendClientMessage(playerid, col_gray,"Игрок не найден.");
	if(params[0] == playerid) return SendClientMessage(playerid, col_gray,"Вы не можете разжаловать самого себя.");
    if(! IsPlayerInRangeOfPoint(playerid, 5, p_t_info[params[0]][p_pos][0], p_t_info[params[0]][p_pos][1], p_t_info[params[0]][p_pos][2])|| GetPlayerVirtualWorld(params[0]) != GetPlayerVirtualWorld(playerid)) return SendClientMessage(playerid, col_gray,"Игрок слишком далеко.");
	if(! army_player(params[0])&& ! cop_player(params[0])) return SendClientMessage(playerid, col_gray,"Игрок не является армейцем или полицейским.");
	if(p_info[params[0]][leader] != 0) return SendClientMessage(playerid, col_gray,"Вы не можете разжаловать лидера организации.");


	if(p_info[params[0]][rank] == 1)
	{
		new scm_string[266];
		format(scm_string, 128, "Агент FBI %s уволил Вас из организации.", p_info[playerid][name]);
		SendClientMessage(params[0], 0x14A3FFFF, scm_string);
		format(scm_string, 128, "Вы уволили %s из организации.", p_info[params[0]][name]);
		SendClientMessage(playerid, 0x14A3FFFF, scm_string);


		format(scm_string,sizeof(scm_string),
		"INSERT INTO `users_jobinfo`(`ji_uid`,`ji_date_invite`,`ji_date_uninvite`,`ji_reason`,`ji_fraction`,`ji_fwarnings`)SELECT `u_id`, `u_fraction_date`, NOW(), 'разжалован','%d','%d' FROM `users` WHERE `u_id` = '%d'",
		p_info[params[0]][member],p_info[params[0]][f_warning], p_info[params[0]][id]);
		mysql_tquery(sql_connection, scm_string, "", "");


		p_info[params[0]][member] = 0;
		p_info[params[0]][rank] = 0;
		p_info[params[0]][org_skin] = 0;
		update_int_sql(params[0], "u_member", p_info[params[0]][member]);
		update_int_sql(params[0], "u_rank", p_info[params[0]][rank]);



		if(is_fraction_duty { params[0]})is_fraction_duty { params[0]} = 0;

		SetPlayerSkin(params[0], p_info[params[0]][skin]);
		SetPlayerColor(params[0], 0xFFFFFF40); // col_white
		return 1;
	}

	p_info[params[0]][rank]--;
	update_int_sql(params[0], "u_rank", p_info[params[0]][rank]);

	new scm_string[128];
	format(scm_string, sizeof(scm_string), "Агент FBI %s понизил вас до %d ранга", p_info[playerid][name], p_info[params[0]][rank]);
	SendClientMessage(params[0], 0x14A3FFFF, scm_string);
	format(scm_string, sizeof(scm_string), "Вы понизили %s до %d ранга.", p_info[params[0]][name], p_info[params[0]][rank]);
	SendClientMessage(playerid, 0x14A3FFFF, scm_string);
	return 1;
}

