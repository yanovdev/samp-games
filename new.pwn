						}


						set_pos(playerid, b_interior[bis_int][i_position][0], b_interior[bis_int][i_position][1], b_interior[bis_int][i_position][2], b_interior[bis_int][i_position][3], b_interior[bis_int][i_interior], b_info[t][b_id]+ 1000);
