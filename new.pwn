stock is_text_invalid(text[])
{
    new length = strlen(text);

    for(new i; i < length; i ++)
    {
        switch(text[i])
        {
            case 'А'..'Я', 'а'..'я', 'A'..'Z', 'a'..'z', '0'..'9', '_', '@', '.', ',', '[', ']', ' ': continue;
            default: return 1;
        }
    }
    return 0;
}

