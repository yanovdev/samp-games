					DeletePVar(playerid, !"factory_table_id");
					ClearAnimations(playerid);
					ApplyAnimation(playerid, !"PED", !"IDLE_tired", 4.1, 0, 1, 1, 0, 1);
					action_td_status(playerid, false);
					return SendClientMessage(playerid, col_white, !""scm_error"Ствол оказалась бракованным. Пожалуйста, повторите попытку.");
				}
				DeletePVar(playerid, !"factory_table_id");
				if(random(100) > 5)
				{
					SetPVarInt(playerid, !"have_factory_box", 2);

					SetPlayerAttachedObject(playerid, 0, 2358,6, 0.0, 0.10, -0.2, -110.0,0.0,78.0);
					SendClientMessage(playerid, col_white, !""scm_info"Вы собрали ствол. Отнесите его на склад.");

					ClearAnimations(playerid);
					ApplyAnimation(playerid, !"PED", !"IDLE_tired", 4.1, 0, 1, 1, 0, 1);
					SetPlayerCheckpoint(playerid, -31.4789, -261.0728, 1528.6615, 2.0);
					return SetPlayerSpecialAction(playerid, SPECIAL_ACTION_CARRY);
				}
				else
				{
					ClearAnimations(playerid);
					ApplyAnimation(playerid, !"PED", !"IDLE_tired", 4.1, 0, 1, 1, 0, 1);
					return SendClientMessage(playerid, col_white, !""scm_error"Ствол оказалась бракованным. Пожалуйста, повторите попытку.");
				}
			}
		}
		return 1;
	}
	switch(random(3))
	{
		case 0:
		{
			SetPVarInt(playerid, !"KeyJob", 65536);
			PlayerTextDrawSetString(playerid, TexDrawJobProgress[playerid][3], "PRESS: ~g~Y");
		}
		case 1:
		{
			SetPVarInt(playerid, !"KeyJob", 131072);
			PlayerTextDrawSetString(playerid, TexDrawJobProgress[playerid][3], "PRESS: ~r~N");
		}
		case 2:
		{
			SetPVarInt(playerid, !"KeyJob", 262144);
			PlayerTextDrawSetString(playerid, TexDrawJobProgress[playerid][3], "PRESS: ~r~H");
		}
	}
	return 1;
}

