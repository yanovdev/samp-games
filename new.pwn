cmd:call(playerid, params[])
{
	if(GetPVarInt(playerid, !"PlayerCuffed")) return SendClientMessage(playerid, col_white, !""scm_error"Вы в наручниках.");
	if(GetPVarInt(playerid, !"PlayerTied")) return SendClientMessage(playerid, col_white, !""scm_error"Вы связаны.");
	if(!p_info[playerid][number]) return SendClientMessage(playerid, col_white, !""scm_error"У Вас нет телефона.");
	if(!p_t_info[playerid][phone_toggled]) return SendClientMessage(playerid, col_white, !""scm_error"У Вас отключен мобильный телефон.");
	if(p_t_info[playerid][phone_id] != INVALID_PLAYER_ID || is_ether_calling{playerid} != 0) return SendClientMessage(playerid, col_white, !""scm_error"Закончите текущий разговор.");
	if(p_info[playerid][phone_balance] < 1) return SendClientMessage(playerid, col_white, !""scm_error"Баланс вашего телефона равен нулю.");
	if(is_ether{playerid} != 0) return SendClientMessage(playerid, col_white, !""scm_error"Вы не можете говорить по телефону во время эфира.");
	extract params -> new call_number; else return SendClientMessage(playerid, col_gray, !"Используйте: "c_white"/call [номер]");
	if(p_info[playerid][number] == call_number) return SendClientMessage(playerid, col_white, !""scm_error"Вы не можете звонить самому себе.");
	if(call_number == 0) return SendClientMessage(playerid, col_white, !""scm_error"Данный номер не существует.");

	if(call_number == 60 || call_number == 060)
	{
		new year, month, day,
			hour, minute, second,
			mtext[11];

		getdate(year, month, day);
		gettime(hour, minute, second);
		FixHour(hour);
		hour = global_hour;

		switch(month)
		{
			case 1: mtext = "января";
			case 2: mtext = "февраля";
			case 3: mtext = "марта";
			case 4: mtext = "апреля";
			case 5: mtext = "мая";
			case 6: mtext = "июня";
			case 7: mtext = "июля";
			case 8: mtext = "августа";
			case 9: mtext = "сентября";
			case 10: mtext = "октября";
			case 11: mtext = "ноября";
			case 12: mtext = "декабря";
		}

		new dialog_string[278];
		if(p_info[playerid][jailed] > 0)
		{
			format(dialog_string, sizeof(dialog_string),"\
			"c_gold"Вы позвонили в службу точного времени.\n\n\
			"c_white"Сегодняшняя дата: "c_silver"%d %s 2020 г\n\
			"c_white"Текущее время %02d:%02d\n\n\
			"c_white"Время заключения: %s",
			day, mtext, hour, minute, convert_time(p_info[playerid][jailed]));
		}
		else
		{
			format(dialog_string, sizeof(dialog_string),"\
			"c_gold"Вы позвонили в службу точного времени.\n\n\
			"c_white"Сегодняшняя дата: "c_silver"%d %s 2020 г\n\
			"c_white"Текущее время %02d:%02d",
			day, mtext, hour, minute);
		}
		show_dialog(playerid, d_none, DIALOG_STYLE_MSGBOX, !""scm_dialog"Служба точного времени", dialog_string, !"Закрыть", !"");
		return me_action(playerid, "позвонил(-а) в службу точного времени");
	}

	if(call_number == 911) return show_dialog(playerid, d_service, DIALOG_STYLE_LIST, !""scm_dialog"Сервисы", !"Полиция\nСкорая помощь\nТакси\nМеханик\nДоставка продуктов", !"Выбрать", !"Назад");

	if(call_number == f_info[25][f_materials] && cas_price[0] != 0)
	{
		new dialog_string[100];
		format(dialog_string, sizeof(dialog_string), ""c_white"Стоимость звонка: "c_green"%d$\n"c_white"Вы действительно хотите позвонить?", cas_price[0]);
		show_dialog(playerid, d_ether_calling, DIALOG_STYLE_MSGBOX, !""scm_dialog"Звонок на прямой эфир", dialog_string, !"Далее", !"Отмена");

		is_ether_calling{playerid} = 26;
		return 1;
	}

	if(call_number == f_info[26][f_materials] && cas_price[1] != 0)
	{
		new dialog_string[100];
		format(dialog_string, sizeof(dialog_string), ""c_white"Стоимость звонка: "c_green"%d$\n"c_white"Вы действительно хотите позвонить?", cas_price[1]);
		show_dialog(playerid, d_ether_calling, DIALOG_STYLE_MSGBOX, !""scm_dialog"Звонок на прямой эфир", dialog_string, !"Далее", !"Отмена");
		
		is_ether_calling{playerid} = 27;
		return 1;
	}

	if(call_number == f_info[27][f_materials] && cas_price[2] != 0)
	{
		new dialog_string[100];
		format(dialog_string, sizeof(dialog_string), ""c_white"Стоимость звонка: "c_green"%d$\n"c_white"Вы действительно хотите позвонить?", cas_price[2]);
		show_dialog(playerid, d_ether_calling, DIALOG_STYLE_MSGBOX, !""scm_dialog"Звонок на прямой эфир", dialog_string, !"Далее", !"Отмена");

		is_ether_calling{playerid} = 28;
		return 1;
	}

	foreach(new i: logged_players)
	{
		if(p_info[i][number] == call_number)
		{
			if(!p_t_info[i][phone_toggled]) return SendClientMessage(playerid, col_white, !""scm_error"У игрока отключен мобильный телефон.");
			if(p_t_info[i][phone_id] != INVALID_PLAYER_ID) return SendClientMessage(playerid, col_white, !""scm_error"Абонент в данный момент уже с кем-то разговаривает.");
			
			new _text_string[144];
            format(_text_string, sizeof(_text_string), ""scm_info"Исходящий вызов | Номер: "c_gold"%d"c_white" | Ожидание ответа от "c_gold"%s", p_info[i][number], p_info[i][name]);
            SendClientMessage(playerid, col_white, _text_string);

			format(_text_string, sizeof(_text_string), ""scm_info"Входящий звонок | Номер: "c_gold"%d"c_white" | Вызывает: "c_gold"%s", p_info[playerid][number], p_info[playerid][name]);
			SendClientMessage(i, col_white, _text_string);
			SendClientMessage(i, col_white, !""scm_info"Используйте "c_green"/p"c_white" чтобы ответить или "c_orange_red"/h"c_white" чтобы отклонить вызов");

			SetPlayerSpecialAction(playerid, SPECIAL_ACTION_USECELLPHONE);
			SetPlayerAttachedObject(playerid, 3, 18870, 6, 0.09, 0.019, -0.011, 88.90, -176.40, 0.49, 0.787, 0.753, 0.562);

			p_t_info[playerid][phone_caller] = 1;
			p_t_info[i][phone_id] = playerid;
			p_t_info[i][phone_caller] = 1;
			return 1;
		}
	}
	return 1;
}

