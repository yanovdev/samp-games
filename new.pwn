			order_price;

		cache_get_value_name_int(0, "bo_products", order_products);
		cache_get_value_name_int(0, "bo_price", order_price);

		if(veh_info[player_vehicle[playerid] - 1][v_cargo] >= order_products)
		{
			veh_info[player_vehicle[playerid] - 1][v_cargo] -= order_products;
			b_info[is_gps_used { playerid } - 21][b_product] += order_products;
			new string[118];
			format(string, sizeof(string), "Вы разгрузили "c_green"%d/5000 кг."c_white" товара за "c_green"%d$"c_white".", order_products, order_products * order_price);
