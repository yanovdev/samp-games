public OnPlayerEditAttachedObject(playerid, response, index, modelid, boneid, Float:fOffsetX, Float:fOffsetY, Float:fOffsetZ, Float:fRotX, Float:fRotY, Float:fRotZ, Float:fScaleX, Float:fScaleY, Float:fScaleZ)
{
	if(p_info[playerid][acces_edit])
	{
		if(response)
		{
			if(fScaleX > 2.0 || fScaleY > 2.0 || fScaleZ > 2.0)
			{
				SendClientMessage(playerid, col_gray, "[X]Не допустимые занчения. Уменьшите придмет до нормального размера");
				return EditAttachedObject(playerid , index);
			}
			player_attach[playerid][index - 5][0] = fOffsetX;
			player_attach[playerid][index - 5][1] = fOffsetY;
			player_attach[playerid][index - 5][2] = fOffsetZ;
			player_attach[playerid][index - 5][3] = fRotX;
			player_attach[playerid][index - 5][4] = fRotY;
			player_attach[playerid][index - 5][5] = fRotZ;
			player_attach[playerid][index - 5][6] = fScaleX;
			player_attach[playerid][index - 5][7] = fScaleY;
			player_attach[playerid][index - 5][8] = fScaleZ;

			new	query_string[256],
				count;
			format(query_string, sizeof(query_string),"SELECT * FROM `attachplayer` WHERE `usid` = %d AND `modelid` = %d", p_info[playerid][id], modelid);
			new Cache:result = mysql_query(sql_connection, query_string);
			count = cache_num_rows();
			cache_delete(result);
			if(count)
			{
				format(query_string,sizeof(query_string),"UPDATE `attachplayer` SET `cor`='%.3f,%.3f,%.3f,%.3f,%.3f,%.3f,%.3f,%.3f,%.3f' WHERE `usid` = %d AND `modelid` = %d", fOffsetX, fOffsetY, fOffsetZ, fRotX, fRotY, fRotZ, fScaleX, fScaleY, fScaleZ, p_info[playerid][id], modelid);
				mysql_tquery(sql_connection, query_string, "", "");
			}
			else
			{
				format(query_string,sizeof(query_string),"INSERT INTO `attachplayer`(usid, bone, modelid, cor)VALUES(%d, %d, %d, '%.3f,%.3f,%.3f,%.3f,%.3f,%.3f,%.3f,%.3f,%.3f')", p_info[playerid][id], boneid, modelid, fOffsetX, fOffsetY, fOffsetZ, fRotX, fRotY, fRotZ, fScaleX, fScaleY, fScaleZ);
				mysql_tquery(sql_connection, query_string, "", "");
			}

			if(GetPVarInt(playerid ,  "Inv: Item") >= 0)
			{
				take_item(playerid , GetPVarInt(playerid ,  "Inv: Item"));
				p_info[playerid][accessories][index -5] = GetPVarInt(playerid ,  "Inv: Item");
			}

			if(isMaskPlayer(modelid))enMaskPlayer(playerid);

			p_info[playerid][acces_edit] = 0;
			return show_player_inventory(playerid);
		}
		else
		{
			p_info[playerid][acces_edit] = 0;
			if(p_info[playerid][accessories][index -5] > 0 && IsPlayerAttachedObjectSlotUsed(playerid , index))
			{
				new a = index -5;
				RemovePlayerAttachedObject(playerid , index);
				return SetPlayerAttachedObject(playerid , index , modelid , boneid , player_attach[playerid][a][0], player_attach[playerid][a][1], player_attach[playerid][a][2], player_attach[playerid][a][3], player_attach[playerid][a][4], player_attach[playerid][a][5], player_attach[playerid][a][6], player_attach[playerid][a][7], player_attach[playerid][a][8]);
			}
			else if(p_info[playerid][accessories][index -5] == 0) return RemovePlayerAttachedObject(playerid , index);
		}
		return 1;
	}
	return 1;
}

stock isMaskPlayer(modelid)
{
	if(modelid >= 18911 && modelid <= 18920) return 1;
	return 0;
}

stock enMaskPlayer(playerid)
{
	if(GetPVarInt(playerid ,"p_masked")) return 1;
	SetPlayerColor(playerid , 0x7a766700);
	SendClientMessage(playerid, col_white, "Вы надели маску. Ваше местоположение скрыто. Используйте "c_green"/unmask"c_white", чтобы снять маску.");
  	if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && ! GetPVarInt(playerid, "PlayerCuffed")&& ! GetPVarInt(playerid, "PlayerTied")) ApplyAnimation(playerid, "shop","ROB_Shifty",4.0,0,0,0,0,0,1);
	SetPlayerChatBubble(playerid , "Надевает маску" , 0xDD90FFFF , 30.0 , 10000);
	SetPVarInt(playerid , "p_masked" , 1);
	return 1;
}

stock unMaskPlayer(playerid)
{
	if(! GetPVarInt(playerid,"p_masked")) return 1;
 	if(! p_info[playerid][member])SetPlayerColor(playerid, 0xFFFFFF40); // col_white
	if(p_info[playerid][member])SetPlayerColor(playerid, f_info[p_info[playerid][member] - 1][f_radar_color]);
	if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && ! GetPVarInt(playerid, "PlayerCuffed")&& ! GetPVarInt(playerid, "PlayerTied"))ApplyAnimation(playerid, "shop","ROB_Shifty",4.0,0,0,0,0,0,1);
	DeletePVar(playerid, "p_masked");
	return 1;
}

