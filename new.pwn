		if(IsPlayerInRangeOfPoint(playerid, 3.5, h_info[h][h_pos][0], h_info[h][h_pos][1], h_info[h][h_pos][2]))
		{
			if(!h_info[h][h_rent_price] || ! h_info[h][h_rent_status]) return SendClientMessage(playerid, col_white, !""scm_error"Данный дом не сдается.");
			if(p_info[playerid][house] != -1 || p_info[playerid][rent_house] != -1) return SendClientMessage(playerid, col_white, !""scm_error"Вы снимаете дом или у Вас уже есть собственный дом.");
			if(p_info[playerid][money] < h_info[h][h_rent_price]) return SendClientMessage(playerid, col_white, !""scm_error"У Вас недостаточно средств.");

			p_info[playerid][rent_house] = h;
			update_int_sql(playerid, "u_renthouse", p_info[playerid][rent_house]);

			new _text_string[100];
			format(_text_string, sizeof(_text_string), ""scm_info"Вы успешно арендовали дом. Стоимость аренды дома в час: "c_green"%d$", h_info[h][h_rent_price]);
			return SendClientMessage(playerid, col_white, _text_string);
		}
	}
	return 1;
}

