stock memfill_byaddr(address, cellsize, value)
{
    new idx;

    #emit LCTRL       6
    #emit MOVE.alt
    #emit LCTRL       0

    #emit ADD
    #emit MOVE.alt

    #emit LCTRL       1
    #emit SUB.alt
    #emit ADD.C       92

    #emit STOR.S.pri  idx
    #emit LOAD.S.pri  cellsize
    #emit SHL.C.pri   2
    #emit SREF.S.pri  idx

    #emit LOAD.S.alt  address
    #emit LOAD.S.pri  value
    #emit FILL        0
}


