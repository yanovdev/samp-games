stock hide_player_inventory(playerid)
{
	if(IsValidVehicle(GetPlayerVehicleID(playerid)))speedometr_status(playerid , true);
	if(add_section[playerid])hide_add_section(playerid);
	for(new td = 0; td < 12; td ++)TextDrawHideForPlayer(playerid , td_inventory[td]);
	for(new td = 0; td < 40; td ++)PlayerTextDrawHide(playerid , td_inventory_p[playerid][td]);
	inv_active[playerid] = 0;
	TogglePlayerControllable(playerid , true);
	return CancelSelectTextDraw(playerid);
}

stock show_player_inventory(playerid , type = 0)// 0 - полный текстдрав		1 - только слоты
{
	if(inv_active_one[playerid] == 0)create_td_inv(playerid);
	if(IsValidVehicle(GetPlayerVehicleID(playerid)))speedometr_status(playerid , false);
	new item ,
		Float: count_i ,
		src[30];
	for(new i = 0; i < 30; i ++)
	{
		new td = i + 1;
		if(p_info[playerid][inv][i] > 0)
		{
			item = p_info[playerid][inv][i];
			PlayerTextDrawFont(playerid, td_inventory_p[playerid][td], 5);
			PlayerTextDrawSetPreviewModel(playerid, td_inventory_p[playerid][td], inv_info[item][i_object]);
			PlayerTextDrawSetPreviewRot(playerid, td_inventory_p[playerid][td], inv_info[item][i_cor][0], inv_info[item][i_cor][1], inv_info[item][i_cor][2], inv_info[item][i_cor][3]);
			PlayerTextDrawColor(playerid, td_inventory_p[playerid][td], 0xFFFFFFFF);
			PlayerTextDrawBackgroundColor(playerid, td_inventory_p[playerid][td], 1061109759);

			if(item >= 123 && item <= 131)count_i += inv_info[item][i_count]+(inv_info[135][i_count]* p_info[playerid][inv_count][i]);
			else count_i += inv_info[item][i_count]* p_info[playerid][inv_count][i];
		}
		else
		{
			PlayerTextDrawFont(playerid , td_inventory_p[playerid][td], 4);
			PlayerTextDrawColor(playerid , td_inventory_p[playerid][td], 1061109759);
		}
	}

