			}
			format(line_string, sizeof(line_string), ""c_white"%s — %s — %s — "c_green"В сети\n", f_info[i][f_leader_name], f_info[i][f_name], f_rank[f_info[i][f_id] - 1][f_info[i][f_rank_settings][0] - 1]);
