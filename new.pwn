stock show_trns(playerid)
{
	if(! trns_active_one[playerid])create_trns(playerid);

	new query_string[50];
	format(query_string , sizeof(query_string), "__transfer to : %s", p_info[GetPVarInt(playerid , "TRNS:id")][name]);
	PlayerTextDrawSetString(playerid, TextDrawTransferP[playerid][0], query_string);

	PlayerTextDrawSetString(playerid, TextDrawTransferP[playerid][11], "__0~g~$");
	PlayerTextDrawSetString(playerid, TextDrawTransferP[playerid][12], "__0~g~$");

	for(new td = 0; td < 3; td ++)TextDrawShowForPlayer(playerid , TextDrawTransfer[td]);
	for(new td = 0; td < 13; td ++)
	{
		if(td > 0 && td < 11)
		{
			PlayerTextDrawFont(playerid , TextDrawTransferP[playerid][td], 4);
			PlayerTextDrawColor(playerid, TextDrawTransferP[playerid][td], 1061109759);
		}
		PlayerTextDrawShow(playerid , TextDrawTransferP[playerid][td]);
	}
	trns_active[playerid] = 1;
	return 1;
}

stock hide_trns(playerid)
{
	for(new td = 0; td < 3; td ++)TextDrawHideForPlayer(playerid , TextDrawTransfer[td]);
	for(new td = 0; td < 13; td ++)PlayerTextDrawHide(playerid , TextDrawTransferP[playerid][td]);
	trns_active[playerid] = 0;
	trns_player_m[playerid] = 0;
	for(new i = 0; i < 5; i ++)
	{
		trns_player[playerid][i][0] = -1;
		trns_player[playerid][i][1] = -1;
	}
	destriy_trns(playerid);
}

stock cancel_trns(playerid)
{
	for(new i = 0; i < 5; i++)
	{
		if(trns_player[playerid][i][0] <= 0)continue;
		give_item(playerid , trns_player[playerid][i][0], trns_player[playerid][i][1]);
	}

	hide_trns(playerid);

	trns_actives[playerid] = -1;
	return 1;
}


stock is_one_item(playerid , item)
{
	new type = inv_info[item][i_type];
	for(new a = 0; a < 5; a++)
	{
		if(p_info[playerid][accessories][a] <= 0)continue;
