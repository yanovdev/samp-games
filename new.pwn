			SendClientMessage(playerid, col_white, !""scm_info"На мини-карте отмечена точка аренды, отправляйтесь туда для аренды мопеда.");
			SetPVarInt(playerid, !"faggio_info", 1);

			SpawnPlayer(playerid);
		}
		load_phonebook(playerid);

		CancelSelectTextDraw(playerid);
		SetPlayerColor(playerid, 0xFFFFFF40);

		ResetPlayerMoney(playerid);
		GivePlayerMoney(playerid, p_info[playerid][money]);

		for(new w = 0; w <= 12; w++)
		{
			if(p_t_info[playerid][p_gun_slot][w] == 0 || p_t_info[playerid][p_gun_ammo][w] == 0)continue;
			give_weapon(playerid, p_t_info[playerid][p_gun_slot][w], p_t_info[playerid][p_gun_ammo][w]);
			continue;
		}

		update_int_sql(playerid, "u_online", 1);
		p_info[playerid][main_timer] = SetTimerEx("player_timer", 1000, 1, "i", playerid);
		return 1;
	}
	else
	{
		if(GetPVarInt(playerid, !"wrong_password") == 4)
		{
			show_dialog(playerid, d_none, DIALOG_STYLE_MSGBOX, !""scm_dialog"Соединение разорвано", !""c_white"Вы превысили максимально допустимое колличество попыток ввода пароля.", !"Закрыть", !"");
			return kick_player(playerid);
		}
		SetPVarInt(playerid, !"wrong_password", GetPVarInt(playerid, !"wrong_password") + 1);
		SendClientMessage(playerid, col_white, !""scm_error"Вы ввели неверный пароль, попробуйте ещё раз.");
		show_login(playerid);
	}
	return 1;
}

callback: load_user_acces(playerid , a)
{
	new rows = cache_num_rows();

