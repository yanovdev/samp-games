CMD:kickout(playerid, params[])
{
	if(! gov_player(playerid)) return 1;

	if(sscanf(params, "u", params[0])) return SendClientMessage(playerid, col_gray,"Используйте: /kickout[ид/имя]");
	if(! IsPlayerConnected(params[0])) return SendClientMessage(playerid, col_gray,"Игрок не найден.");
	if(params[0] == playerid) return SendClientMessage(playerid, col_gray,"Вы не можете выгнать самого себя.");
    if(! IsPlayerInRangeOfPoint(playerid, 5, p_t_info[params[0]][p_pos][0], p_t_info[params[0]][p_pos][1], p_t_info[params[0]][p_pos][2])|| GetPlayerVirtualWorld(params[0]) != GetPlayerVirtualWorld(playerid)) return SendClientMessage(playerid, col_gray,"Игрок слишком далеко.");
	if(is_tazered { params[0]} == 0) return SendClientMessage(playerid, col_gray,"Игрок должен быть поражён шокером.");
	new scm_string[82];
	format(scm_string, 82, "%s[%d]выгнал Вас из мэрии.",
	p_info[playerid][name], playerid);
	SendClientMessage(params[0], col_wavy, scm_string);

	format(scm_string, 82, "Вы выгнали %s[%d]из мэрии.",
	p_info[params[0]][name], params[0]);
	SendClientMessage(playerid, col_wavy, scm_string);


	kickout_cooldown[params[0]] = gettime()+ 300;

	set_pos(params[0], 1473.2272,-1766.5834,18.7949,326.2043, 0, 0);
	return true;
}
CMD:newveh(playerid, params[])
{
    if(p_info[playerid][admin] < 4) return -1;
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER) return SendClientMessage(playerid, col_gray,"Вы уже за рулём.");
	if(sscanf(params, "ddd", params[0 ], params[1 ], params[2])) return SendClientMessage(playerid, col_gray,"Используйте: /newveh[ид авто][цвет 1][цвет 2]");
	if(params[0] < 400 || params[0] > 611) return SendClientMessage(playerid, col_gray,"Используйте: /newveh[ид авто][цвет 1][цвет 2]");
	if(params[1] < 0 || params[1] > 255) return SendClientMessage(playerid, col_gray,"Используйте: /newveh[ид авто][цвет 1][цвет 2]");
	if(params[2] < 0 || params[2] > 255) return SendClientMessage(playerid, col_gray,"Используйте: /newveh[ид авто][цвет 1][цвет 2]");
	new Float:player_pos[3];
	GetPlayerPos(playerid, player_pos[0], player_pos[1], player_pos[2]);
	new adm_veh = CreateVehicle(params[0], player_pos[0], player_pos[1], player_pos[2], 0.0, params[1], params[2], -1);

	veh_info[adm_veh - 1][v_pos][0] = player_pos[0];
	veh_info[adm_veh - 1][v_pos][1] = player_pos[1];
	veh_info[adm_veh - 1][v_pos][2] = player_pos[2];

	SetPVarInt(playerid, "admin_vehicle", adm_veh);
	veh_info[adm_veh - 1][v_fuel] = 60.0;
	veh_info[adm_veh - 1][v_engine] = 100.0;

	PutPlayerInVehicle(playerid, adm_veh, 0);
	SetPlayerArmedWeapon(playerid,0);
	new scm_string[128];
	format(scm_string, sizeof(scm_string ), ""c_server"A.INFO: "c_grey"%s создал автомобиль. ID: %d | Model: %d", p_info[playerid][name], adm_veh, params[0]);
	foreach(new i: admin_players)SendClientMessage(i, col_gray, scm_string);

	return true;
}

cmd:givedonate(playerid, params[])
{
	if(p_info[playerid][admin] < 8) return 1;
	extract params -> new player:targetid, give_donate; else return SendClientMessage(playerid, col_gray, !"Используйте: "c_white"/givedonate [id игрока] [донат-рубли]");
	if(!(10 <= give_donate <= 1000)) return SendClientMessage(playerid, col_white, !""scm_error"Сумма доната не должна быть меньше 10 и больше 1.000 рублей");
	if(!IsPlayerConnected(targetid)) return SendClientMessage(playerid, col_white, !""scm_error"Игрок не найден.");

	new text_string[130];
	format(text_string, sizeof(text_string), "Вы выдали игроку "c_server"%s"c_white" донат-рубли в рамере %d рублей", p_info[targetid][name], give_donate);
	SendClientMessage(playerid, col_white, text_string);

	new query_string[95];
	mysql_format(sql_connection, query_string, sizeof(query_string), "UPDATE `users` SET `u_donate`=`u_donate`+'%d' WHERE `u_id`='%d' LIMIT 1", give_donate, p_info[targetid][id]);
	mysql_query(sql_connection, query_string);
	return 1;
}

