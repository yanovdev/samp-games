stock useInvItem(playerid , slot)
{
	switch(slot)
	{
		case 1..2, 11..15: // Еда
		{
		 	if(GetPVarInt(playerid, !"PlayerCuffed")) return SendClientMessage(playerid, col_white, !""scm_error"Вы в наручниках.");
			if(GetPVarInt(playerid, !"PlayerTied")) return SendClientMessage(playerid, col_white, !""scm_error"Вы связаны.");
			if(!take_item(playerid, p_info[playerid][inv][slot])) return 0;

			if(p_info[playerid][hunger] + b_cafe_product[random(6)] > 100)
				p_info[playerid][hunger] = 100;
			else
				p_info[playerid][hunger] += b_cafe_product[random(6)];

			PlayerTextDrawTextSize(playerid, td_inventory_p[playerid][38], 1.28 * p_info[playerid][hunger], 13.0000);

			ApplyAnimation(playerid, !"FOOD", !"EAT_Burger", 4.0, 0, 0, 0, 0, 0, 1);
			me_action(playerid, "съел(-а) набор еды");
		}
		case 3: // Телефон
		{
		 	if(GetPVarInt(playerid, !"PlayerCuffed")) return SendClientMessage(playerid, col_white, !""scm_error"Вы в наручниках.");
			if(GetPVarInt(playerid, !"PlayerTied")) return SendClientMessage(playerid, col_white, !""scm_error"Вы связаны.");
			if(!p_info[playerid][number]) return SendClientMessage(playerid, col_white, !""scm_error"У Вас нет телефона.");
			
			global_string[0] = EOS;
			format(global_string, 254, "\
			"c_server"1. "c_white"Позвонить в экстренный службы\n\
			"c_server"2. "c_white"Позвонить на номер\n\
			"c_server"3. "c_white"Отправить сообщение\n\
			"c_server"4. "c_white"Список контактов\n\
			"c_server"5. "c_white"Добавить контакт\n\
			"c_server"6. %s "c_white"телефон",
			(p_t_info[playerid][phone_toggled]) ? ""c_red"Выключить" : ""c_green"Включить");
			return show_dialog(playerid, d_phone_main, DIALOG_STYLE_LIST, !""scm_dialog"Мобильный телефон", global_string, !"Выбрать", !"Закрыть");
		}
		case 4: return 1;// рация
		case 5: // Справочник
		{
			return show_dialog(playerid, d_direction, DIALOG_STYLE_LIST, ""c_server"Справочник","1. Лидеры организации онлайн\n2. Механики\n3. Таксисты\n4. Репортёры", "Далее", "Отмена");
		}
		case 6: // аптечка
		{
		 	if(GetPVarInt(playerid, !"PlayerCuffed")) return SendClientMessage(playerid, col_white, !""scm_error"Вы в наручниках.");
			if(GetPVarInt(playerid, !"PlayerTied")) return SendClientMessage(playerid, col_white, !""scm_error"Вы связаны.");
			if(TeamPaint[playerid] != 0) return SendClientMessage(playerid, col_white, !""scm_error"Невозможно во время участия в PaintBall.");
		    if(heal_cooldown[playerid] > gettime()) return SendClientMessage(playerid, col_white, !""scm_error"Аптечку можно использовать раз в 5 секунд.");
			if(!take_item(playerid, 6)) return 0;

			new Float:pl_health;
			GetPlayerHealth(playerid, pl_health);

			if(pl_health + 65 > 160)
				set_health(playerid, 160);
			else 
				set_health(playerid, pl_health + 65);

			GameTextForPlayer(playerid, "~g~+65HP", 2000, 1);
			heal_cooldown[playerid] = gettime() + 5;

			if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT && !GetPVarInt(playerid, !"PlayerCuffed") && !GetPVarInt(playerid, !"PlayerTied")) ApplyAnimation(playerid, !"ped", !"gum_eat", 4.0, 0, 0, 0, 0, 0, 1);
			return me_action(playerid, "использовал(-а) аптечку");
		}
		case 7: // Ремонтный набор
		{
		 	if(GetPVarInt(playerid, !"PlayerCuffed")) return SendClientMessage(playerid, col_white, !""scm_error"Вы в наручниках.");
			if(GetPVarInt(playerid, !"PlayerTied")) return SendClientMessage(playerid, col_white, !""scm_error"Вы связаны.");
			if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, col_white, !""scm_error"Вы должны быть за рулём транспортного средства.");
			if(GetVehicleSpeed(player_vehicle[playerid]) > 5) return SendClientMessage(playerid, col_white, !""scm_error"Нельзя использовать ремонтный набор на ходу.");
			if(!take_item(playerid, 7)) return 0;

		    SendClientMessage(playerid, col_white, !""scm_error"Вы успешно починили свой транспорт.");
		    RepairVehicle(player_vehicle[playerid]);

			return me_action(playerid, "отремонтировал(-а) свою машину");
		}
		case 8: // gps трекер
		{
			new veh_id = GetPlayerVehicleID(playerid);
			if(veh_id == 0) return SendClientMessage(playerid, col_white, !""scm_error"Вы должны быть в транспорте.");
			if(veh_info[veh_id - 1][v_type] != vehicle_type_player) return SendClientMessage(playerid, col_white, !""scm_error"GPS-трекер можно установить только на личный транспорт.");
			if(veh_info[veh_id - 1][v_gps_tracker] == 1) return SendClientMessage(playerid, col_white, !""scm_error"GPS-трекер уже установлен.");
			if(!take_item(playerid, 8)) return SendClientMessage(playerid, col_white, !""scm_error"У Вас нет GPS-трекера.");

			veh_info[veh_id - 1][v_gps_tracker] = 1;

			new query_string[75];
			format(query_string, sizeof(query_string), "UPDATE `users_vehicles` SET `v_gps_tracker`='%d' WHERE `v_id`='%d'" , veh_info[veh_id - 1][v_gps_tracker], veh_info[veh_id - 1][v_id]);
			mysql_query(sql_connection, query_string);

			return SendClientMessage(playerid, col_white, !""scm_info"Вы успешно установили GPS-трекер в транспорт.");
		}
		case 9: // GPS
		{
			if(GetPVarInt(playerid, !"PlayerCuffed")) return SendClientMessage(playerid, col_white, !""scm_error"Вы в наручниках.");
			if(GetPVarInt(playerid, !"PlayerTied")) return SendClientMessage(playerid, col_white, !""scm_error"Вы связаны.");
			if(GetPVarInt(playerid, !"exam_vehicle") != 0) return SendClientMessage(playerid, col_white, !""scm_error"Невозможно во время сдачи экзамена.");
			if(p_t_info[playerid][pBusRoute]) return SendClientMessage(playerid, col_white, !""scm_error"Невозможно использовать в данный момент.");
			if(is_gps_used{playerid})
			{
				SendClientMessage(playerid, col_white, !""scm_info"Последняя метка была уничтожена.");
				DisablePlayerRaceCheckpoint(playerid);
				is_gps_used{playerid} = 0;
			}
			return show_gps(playerid);
		}
		case 10: SendClientMessage(playerid, col_white, !""scm_info"Используйте команду "c_gold"/tie"c_white", чтобы связать кого-либо.");
		case 20: // Канистра
		{
			if(GetPVarInt(playerid, !"PlayerCuffed")) return SendClientMessage(playerid, col_white, !""scm_error"Вы в наручниках.");
			if(GetPVarInt(playerid, !"PlayerTied")) return SendClientMessage(playerid, col_white, !""scm_error"Вы связаны.");
			if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, col_white, !""scm_error"Вы должны быть за рулём транспортного средства.");
			if(GetVehicleSpeed(player_vehicle[playerid]) > 5) return SendClientMessage(playerid, col_white, !""scm_error"Нельзя использовать канистру с бензином на ходу.");
			if(!take_item(playerid, 20)) return 1;

			new vehicle_id = GetPlayerVehicleID(playerid)- 1;
			if(veh_info[vehicle_id][v_fuel] > 99) return SendClientMessage(playerid, col_white, !""scm_error"Бак транспорт полон.");
			veh_info[vehicle_id][v_fuel] += 15;
			if(veh_info[vehicle_id][v_fuel] > 100) veh_info[vehicle_id][v_fuel] = 100;

		    SendClientMessage(playerid, col_white, !""scm_info"Вы успешно заправили свой транспорт.");
			return me_action(playerid, "заправил(-а) свою машину");
		}
		case 123..131: show_dialog(playerid, d_armoury_patr_in, DIALOG_STYLE_INPUT, !""scm_dialog""c_server"Заряд патронами | "c_white"Количество патрон", !""c_white"Укажите количество патрон которое хотите зарядить в оружие.\n"c_grey"Оставьте поле пустым если заряжать патронами не надо", !"Далее", !"Отмена");
	}
	return 1;
}

