	format(scm_string, 144, "%s[%d]выписал(-а)Вам штраф в размере %d$. Причина: %s.",
	p_info[playerid][name], playerid, params[1], params[2]);
	SendClientMessage(params[0], col_wavy, scm_string);
	return 1;
}


CMD:search(playerid, params[])
{
	if(! cop_player(playerid)&& ! fbi_player(playerid)) return 1;
	if(sscanf(params, "u", params[0])) return SendClientMessage(playerid, col_gray,"Используйте: /search[ид/имя]");
	if(! IsPlayerConnected(params[0])) return SendClientMessage(playerid, col_gray,"Игрок не найден.");
	if(params[0] == playerid) return SendClientMessage(playerid, col_gray,"Вы не можете обыскать самого себя.");
 	if(p_info[params[0]][wanted] == 0) return SendClientMessage(playerid, col_gray,"Данный игрок не находится в розыске.");

	if(is_gps_used { playerid })
	{
		SendClientMessage(playerid, col_white, "[GPS]Последняя метка уничтожена.");
		DisablePlayerRaceCheckpoint(playerid);
		is_gps_used { playerid } = 0;
	}
	else
	{
		new scm_string[100];
		format(scm_string, sizeof(scm_string), "Вы отметили на карте "c_green"%s[%i]"c_white".", p_info[params[0]][name], params[0]);
		SendClientMessage(playerid, col_white, scm_string);

		DisablePlayerRaceCheckpoint(playerid);
		SetPlayerRaceCheckpoint(playerid, 1, p_t_info[params[0]][p_pos][0], p_t_info[params[0]][p_pos][1], p_t_info[params[0]][p_pos][2], 0.0, 0.0, 0.0, 5);
		is_gps_used { playerid } = 1;
	}
	return 1;
}










CMD:m(playerid, params[])
{
	if(! cop_player(playerid)&& p_info[playerid][member] != 7) return 1;
	if(p_info[playerid][mute]) return SendClientMessage(playerid, col_white, !""scm_error"Доступ в чат заблокирован. Время до разблокировки: "c_server"/time");
	if(isnull(params)|| params[0] == ' ') return SendClientMessage(playerid, col_gray,"Используйте: /m[текст]");

	new veh_id = GetPlayerVehicleID(playerid);
	if(veh_id == 0) return SendClientMessage(playerid, col_gray,"Нужно находиться в транспорте.");

	if(veh_info[veh_id - 1][v_type] == vehicle_type_server &&(veh_info[veh_id - 1][v_owner] == 4 || veh_info[veh_id - 1][v_owner] == 5 || veh_info[veh_id - 1][v_owner] == 6 || veh_info[veh_id - 1][v_owner] == 7))
	{
		new scm_string[144];
		format(scm_string, sizeof(scm_string),"[M]%s %s(%d): %s",
		f_rank[p_info[playerid][member] - 1][p_info[playerid][rank] - 1], p_info[playerid][name], playerid, params);
		send_world_message(playerid, 30.0, scm_string, col_yellow, col_yellow, col_yellow, false);
	}
	return true;
}

cmd:su(playerid, params[])
{
    if(!cop_player(playerid) && p_info[playerid][member] != 7) return 1;
    if(p_info[playerid][rank] < 2) return SendClientMessage(playerid, col_white, !""scm_error"Данная команда доступна с 2 ранга.");
    extract params -> new player:targetid, su_wanted, string:su_reason[64]; else return SendClientMessage(playerid, col_gray, !"Используйте: "c_white"/su [id игрока] [розыск] [причина]");
    if(!IsPlayerConnected(targetid) || targetid == playerid) return SendClientMessage(playerid, col_white, !""scm_error"Игрок не найден.");
    if(su_wanted < 1 || su_wanted > 6) return SendClientMessage(playerid, col_white, !""scm_error"Уровень розыска должен быть от 1 до 6.");
    if(p_info[playerid][jailed] > 0) return SendClientMessage(playerid, col_white, !""scm_error"Нельзя объявить в розыск заключенного.");
    if(pl_afk_time[targetid] > 30) return SendClientMessage(playerid, col_white, !""scm_error"Этот игрок в AFK. Ему нельзя выдать розыск.");
    if(p_info[targetid][wanted] + su_wanted > 6) return SendClientMessage(playerid, col_white, !""scm_error"Вы не можете выдать игроку более 6 звёзд.");

    p_info[targetid][wanted] = su_wanted;
    SetPlayerWantedLevel(targetid, p_info[targetid][wanted]);
    update_int_sql(targetid, "u_wanted", p_info[targetid][wanted]);

    new text_string[144];
    format(text_string, sizeof(text_string), "%s %s объявил Вас в розыск. Причина: %s. Текущий уровень розыска: %d", f_rank[p_info[playerid][member] - 1][p_info[playerid][rank] - 1], p_info[playerid][name], su_reason, p_info[targetid][wanted]);
    SendClientMessage(targetid, col_orange_red, text_string);

    format(text_string, sizeof(text_string), "Вы объявили %s в розыск. Причина: %s. Текущий уровень розыска: %d", p_info[targetid][name], su_reason, p_info[targetid][wanted]);
    SendClientMessage(playerid, 0x0099FFFF, text_string);

    format(text_string, sizeof(text_string), "%s %s[%d] объявил %s[%d] в розыск [%d/6], причина: %s", f_rank[p_info[playerid][member] - 1][p_info[playerid][rank] - 1], p_info[playerid][name], playerid, p_info[targetid][name], targetid, p_info[targetid][wanted], su_reason);
    foreach(new i: logged_players) if(cop_player(i) || fbi_player(i)) SendClientMessage(i, 0xFFD700FF, text_string);
    return 1;
}

