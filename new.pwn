CMD:sms(playerid, params[])
{
	if(p_info[playerid][mute]) return SendClientMessage(playerid, col_white, !""scm_error"Доступ в чат заблокирован. Время до разблокировки: "c_server"/time");
	if(GetPVarInt(playerid, "PlayerCuffed")) return SendClientMessage(playerid, col_gray,"Вы в наручниках.");
	if(GetPVarInt(playerid, "PlayerTied")) return SendClientMessage(playerid, col_gray,"Вы связаны.");
	if(! p_info[playerid][number]) return SendClientMessage(playerid, col_gray,"У вас нет телефона.");
	if(! p_t_info[playerid][phone_toggled]) return SendClientMessage(playerid, col_gray,"У вас отключен мобильный телефон.");
	if(p_info[playerid][phone_balance] < 5) return SendClientMessage(playerid, col_gray,"У вас нет средств для отправки SMS.");
	if(sscanf(params, "ds[82]", params[0], params[1])) return SendClientMessage(playerid, col_gray,"Используйте: /sms[номер][текст]");
	if(params[0] == 0) return SendClientMessage(playerid, col_gray,"Данный номер не существует.");
	check_advertise(playerid, params[1]);

	if(params[0] == f_info[25][f_materials] && cas_price[0] != 0)
	{
		if(cas_price[0] > p_info[playerid][phone_balance]) return SendClientMessage(playerid, col_white, !""scm_error"На вашем счету недостаточно средств.");

		p_info[playerid][phone_balance] -= cas_price[0];
		f_info[25][f_money] += cas_price[0];

		new query_string[86];
		format(query_string, sizeof(query_string), "UPDATE `fractions` SET `f_money`='%d' WHERE `f_id`='26' LIMIT 1", f_info[25][f_money]);
		mysql_query(sql_connection, query_string);

		new text_string[144];
		format(text_string, sizeof(text_string), "SMS (эфир) | %s[%d]: %s (тел. %d)", p_info[playerid][name], playerid, params[1], p_info[playerid][number]);
		foreach(new i: logged_players) if(p_info[i][member] == 26 || big_ears{i} == 1) SendClientMessage(i, col_yellow, text_string);
		return SendClientMessage(playerid, col_yellow, text_string);
	}

	if(params[0] == f_info[26][f_materials] && cas_price[1] != 0)
	{
		if(cas_price[1] > p_info[playerid][phone_balance]) return SendClientMessage(playerid, col_white, !""scm_error"На вашем счету недостаточно средств.");

		p_info[playerid][phone_balance] -= cas_price[1];
		f_info[26][f_money] += cas_price[1];

		new query_string[86];
		format(query_string, sizeof(query_string), "UPDATE `fractions` SET `f_money`='%d' WHERE `f_id`='27' LIMIT 1", f_info[26][f_money]);
		mysql_query(sql_connection, query_string);

		new text_string[144];
		format(text_string, sizeof(text_string), "SMS (эфир) | %s[%d]: %s (тел. %d)", p_info[playerid][name], playerid, params[1], p_info[playerid][number]);
		foreach(new i: logged_players) if(p_info[i][member] == 27 || big_ears{i} == 1) SendClientMessage(i, col_yellow, text_string);
		return SendClientMessage(playerid, col_yellow, text_string);
	}

	if(params[0] == f_info[27][f_materials] && cas_price[2] != 0)
	{
		if(cas_price[0] > p_info[playerid][phone_balance]) return SendClientMessage(playerid, col_white, !""scm_error"На вашем счету недостаточно средств.");

		p_info[playerid][phone_balance] -= cas_price[2];
		f_info[27][f_money] += cas_price[2];

		new query_string[86];
		format(query_string, sizeof(query_string), "UPDATE `fractions` SET `f_money`='%d' WHERE `f_id`='26' LIMIT 1", f_info[27][f_money]);
		mysql_query(sql_connection, query_string);

		new text_string[144];
		format(text_string, sizeof(text_string), "SMS (эфир) | %s[%d]: %s (тел. %d)", p_info[playerid][name], playerid, params[1], p_info[playerid][number]);
		foreach(new i: logged_players) if(p_info[i][member] == 28 || big_ears{i} == 1) SendClientMessage(i, col_yellow, text_string);
		return SendClientMessage(playerid, col_yellow, text_string);
	}

	foreach(new i: logged_players)
	{
		if(p_info[i][number] == params[0])
		{
			if(i == playerid) return SendClientMessage(playerid, col_gray,"Нельзя отправить SMS-самому себе.");

			if(! p_t_info[i][phone_toggled])
			{
				return SendClientMessage(playerid, col_gray,"У игрока отключен мобильный телефон.");
			}
			new scm_string[144];

			format(scm_string, sizeof(scm_string), "SMS: %s | Получатель: %s(%i)[т. %i]", params[1], p_info[i][name], i, p_info[i][number]);
			SendClientMessage(playerid, col_yellow, scm_string);
			format(scm_string, sizeof(scm_string), "SMS: %s | Отправитель: %s(%i)[т. %i]", params[1], p_info[playerid][name], playerid, p_info[playerid][number]);
			SendClientMessage(i,  col_yellow, scm_string);

			format(scm_string, sizeof(scm_string), "SMS: %s(%d)to %s(%d)| %s", p_info[playerid][name], playerid, p_info[i][name], i, params[1]);
			foreach(new j: admin_players)
			{
				if(big_ears { playerid } == 0)continue;
				SendClientMessage(j,  col_yellow, scm_string);
			}
			PlayerPlaySound(i, 1052, 0.0, 0.0, 0.0);
			PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
			p_info[playerid][phone_balance]--;
			return 1;
		}
	}
	SendClientMessage(playerid, col_gray, "В настоящий момент абонент недоступен!");
	return 1;
}
