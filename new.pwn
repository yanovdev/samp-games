			}
		}
	}
	return 1;
}

public OnUnoccupiedVehicleUpdate(vehicleid, playerid, passenger_seat, Float:new_x, Float:new_y, Float:new_z, Float:vel_x, Float:vel_y, Float:vel_z)
{
    #if defined debug_mode
        printf("[DEBUG][OnUnoccupiedVehicleUpdate] vehicleid:%d | playerid:%d | passenger_seat:%d", vehicleid, playerid, passenger_seat);
    #endif
    
    if(passenger_seat)
    {
        p_t_info[playerid][last_unoccup_tick] = GetTickCount();
        return 0;
    }
    
    veh_info[vehicleid - 1][v_velocity][0] = vel_x;
    veh_info[vehicleid - 1][v_velocity][1] = vel_y;
    return 1;
}

public OnPlayerClickPlayerTextDraw(playerid, PlayerText:playertextid)
{
	if(p_t_info[playerid][p_dialog] != -1) return 1;


	if(inv_active[playerid])
	{
		if(add_section[playerid])
		{
			new slot = GetPVarInt(playerid , "Inv: Select"),
				g ,
				item ,
				items,
				sec;
			for(new td = 41; td < 56; td ++)
			{
				if(playertextid != td_inventory_p[playerid][td])continue;
				g = td - 41;
				sec = (player_garb_area[playerid] >= 0) ? (player_garb_area[playerid]) : (player_veh_area[playerid]);

				if(slot >= 0)// положить в багажник . мусорку
				{
					if(player_garb_area[playerid] >= 0)
					{
						if(garb_info[sec][g_item][g] == 0)
						{
							garb_info[sec][g_item][g] = p_info[playerid][inv][slot];
							garb_info[sec][g_item_count][g] = p_info[playerid][inv_count][slot];
						}
						else if(garb_info[sec][g_item][g] == p_info[playerid][inv][slot])
						{
							garb_info[sec][g_item_count][g] += p_info[playerid][inv_count][slot];
						}
						else if(garb_info[sec][g_item_count][g] != p_info[playerid][inv_count][slot]) return 1;

						items = p_info[playerid][inv][slot];
					}
					else if(player_veh_area[playerid] >= 0)
					{
						if(!v_boot_a(sec)) return SendClientMessage(playerid, col_gray, !""scm_error"В машине закрыт багажник. Откройте его для загрузки предметов");

						if(slot >= 30)
						{
							if(slot == (g + 30))
							{
								PlayerTextDrawBackgroundColor(playerid, td_inventory_p[playerid][td], 1061109759);
								SetPVarInt(playerid, !"Inv: Select", -1);
								PlayerTextDrawShow(playerid, td_inventory_p[playerid][td]);
								return 1;
							}
							if(slot != (g + 30))
							{
								if(veh_info[sec][v_item][g] <= 0)
								{
									slot -= 30;

									veh_info[sec][v_item][g] = veh_info[sec][v_item][slot];
									veh_info[sec][v_item_count][g] = veh_info[sec][v_item_count][slot];

									item = veh_info[sec][v_item][g];

									veh_info[sec][v_item][slot] = 0;
									veh_info[sec][v_item_count][slot] = 0;

									PlayerTextDrawFont(playerid, td_inventory_p[playerid][td], 5);
									PlayerTextDrawSetPreviewModel(playerid, td_inventory_p[playerid][td], inv_info[item][i_object]);
									PlayerTextDrawSetPreviewRot(playerid, td_inventory_p[playerid][td], inv_info[item][i_cor][0], inv_info[item][i_cor][1], inv_info[item][i_cor][2], inv_info[item][i_cor][3]);
									PlayerTextDrawColor(playerid, td_inventory_p[playerid][td], 0xFFFFFFFF);
									PlayerTextDrawBackgroundColor(playerid, td_inventory_p[playerid][td], 1061109759);

									PlayerTextDrawShow(playerid, td_inventory_p[playerid][td]);

									td = slot + 11 + 30;

									PlayerTextDrawFont(playerid, td_inventory_p[playerid][td], 4);
									PlayerTextDrawColor(playerid, td_inventory_p[playerid][td], 1061109759);
									PlayerTextDrawShow(playerid, td_inventory_p[playerid][td]);

									SetPVarInt(playerid, !"Inv: Select", -1);
									update_veh_ammount(playerid, sec);
									return 1;
								}
							}
						}
						else if(slot < 30)
						{
							if(veh_info[sec][v_item][g] <= 0)
							{
								veh_info[sec][v_item][g] = p_info[playerid][inv][slot];
								veh_info[sec][v_item_count][g] = p_info[playerid][inv_count][slot];
							}
							if(veh_info[sec][v_item][g] == p_info[playerid][inv][slot])
							{
								veh_info[sec][v_item_count][g] += p_info[playerid][inv_count][slot];
							}
							else if(veh_info[sec][v_item][g] != p_info[playerid][inv][slot]) return 1;

							items = p_info[playerid][inv][slot];
							update_veh_ammount(playerid , sec);
						}
					}

					item = p_info[playerid][inv][slot];
					take_item(playerid, p_info[playerid][inv][slot], p_info[playerid][inv_count][slot], slot);

					PlayerTextDrawFont(playerid, td_inventory_p[playerid][td], 5);
					PlayerTextDrawSetPreviewModel(playerid, td_inventory_p[playerid][td], inv_info[item][i_object]);
					PlayerTextDrawSetPreviewRot(playerid, td_inventory_p[playerid][td], inv_info[item][i_cor][0], inv_info[item][i_cor][1], inv_info[item][i_cor][2], inv_info[item][i_cor][3]);
					PlayerTextDrawColor(playerid, td_inventory_p[playerid][td], 0xFFFFFFFF);
					PlayerTextDrawBackgroundColor(playerid, td_inventory_p[playerid][td], 1061109759);
					PlayerTextDrawShow(playerid, td_inventory_p[playerid][td]);

					new _t_string[128];
					format(_t_string, sizeof(_t_string), "Положил в %s \"%s\"", (player_garb_area[playerid] >= 0) ? ("мусорку") : ("багажник машины"), inv_info[items][i_name]);
					SetPlayerChatBubble(playerid, _t_string, col_light_purple, 13, 5000);
					return 1;
				}
				else if(slot < 0) // достать из багажника / мусоркиse
				{
					if(player_garb_area[playerid] >= 0)
					{
						if(garb_info[sec][g_item][g] <= 0) return 1;
						if(!give_item(playerid, garb_info[sec][g_item][g], garb_info[sec][g_item_count][g])) return 0;
						
						garb_info[sec][g_item][g] = 0;
						garb_info[sec][g_item_count][g] = 0;

						PlayerTextDrawFont(playerid, td_inventory_p[playerid][td], 4);
						PlayerTextDrawColor(playerid, td_inventory_p[playerid][td], 1061109759);
						PlayerTextDrawShow(playerid, td_inventory_p[playerid][td]);
					}
					else if(player_veh_area[playerid] >= 0)
					{
						if(!v_boot_a(player_veh_area[playerid])) return SendClientMessage(playerid, col_white, !""scm_error"В машине закрыт багажник. Откройте его для загрузки предметов");
						if(veh_info[sec][v_item][g] <= 0) return 1;

						SetPVarInt(playerid, !"Inv: Select", g + 30);
						PlayerTextDrawBackgroundColor(playerid, td_inventory_p[playerid][td], 0xFFFFE0FF);
						PlayerTextDrawShow(playerid, td_inventory_p[playerid][td]);
						return 1;
					}
					return 1;
				}
			}
		}
		if(trns_active[playerid])
		{
			if(playertextid == TextDrawTransferP[playerid][12])// cancel
			{
				new query_string[200];
				format(query_string , sizeof(query_string), "{ffffff}Введите сумму для обмена\nСумма обмена составляет: "c_green"%d $\n\n"c_grey"Cумма должна быть не менее 1000$ и не более 30.000.000$", trns_player_m[playerid]);
				return show_dialog(playerid , d_trns_money , DIALOG_STYLE_INPUT , ""c_server"Установка цены обмена", query_string , "Далее" , "Отмена");
			}
			for(new td = 6; td < 11; td ++)
			{
				new s = td - 6 ,
					slot = GetPVarInt(playerid , "Inv: Select");
				if(playertextid != TextDrawTransferP[playerid][td])continue;
				if(slot >= 0)
				{
					if(trns_player[playerid][s][0] <= 0)
					{
						trns_player[playerid][s][0] = p_info[playerid][inv][slot];
						trns_player[playerid][s][1] = p_info[playerid][inv_count][slot];

						new item = p_info[playerid][inv][slot];

						take_item(playerid , p_info[playerid][inv][slot], p_info[playerid][inv_count][slot]);

						PlayerTextDrawFont(playerid, TextDrawTransferP[playerid][td], 5);
						PlayerTextDrawSetPreviewModel(playerid, TextDrawTransferP[playerid][td], inv_info[item][i_object]);
						PlayerTextDrawSetPreviewRot(playerid, TextDrawTransferP[playerid][td], inv_info[item][i_cor][0], inv_info[item][i_cor][1], inv_info[item][i_cor][2], inv_info[item][i_cor][3]);
						PlayerTextDrawColor(playerid, TextDrawTransferP[playerid][td], 0xFFFFFFFF);
						PlayerTextDrawBackgroundColor(playerid, TextDrawTransferP[playerid][td], 1061109759);

						PlayerTextDrawShow(playerid , TextDrawTransferP[playerid][td]);

						td -= 5;

						new players = trns_actives[playerid];

						PlayerTextDrawFont(players, TextDrawTransferP[players][td], 5);
						PlayerTextDrawSetPreviewModel(players, TextDrawTransferP[players][td], inv_info[item][i_object]);
						PlayerTextDrawSetPreviewRot(players, TextDrawTransferP[players][td], inv_info[item][i_cor][0], inv_info[item][i_cor][1], inv_info[item][i_cor][2], inv_info[item][i_cor][3]);
						PlayerTextDrawColor(players, TextDrawTransferP[players][td], 0xFFFFFFFF);
						PlayerTextDrawBackgroundColor(players, TextDrawTransferP[players][td], 1061109759);

						PlayerTextDrawShow(players , TextDrawTransferP[players][td]);

						return 1;
					}
				}
			}
		}

		for(new td = 33; td < 38; td ++)
		{
			if(playertextid != td_inventory_p[playerid][td])continue;
			new i = td - 33;
			SetPVarInt(playerid , "Inv: Acces", i);
			if(p_info[playerid][accessories][i] > 0)
			{
				if(GetPVarInt(playerid , "Inv: Select") > 0) return 1;
				return show_dialog(playerid , d_access_edit , DIALOG_STYLE_LIST , ""c_server"Аксессуары | "c_white"Действие" , ""c_white"1)Снять\n2)Редактировать" , "Выбор", "Отмена");
			}
			else
			{
				if(GetPVarInt(playerid , "Inv: Select") < 0) return 1;
				new slot = GetPVarInt(playerid , "Inv: Select");
				if(is_one_item(playerid , p_info[playerid][inv][slot])) return SendClientMessage(playerid,col_gray,"Аксессуар такого типа уже надет на Вас.");

				if(inv_info[p_info[playerid][inv][slot]][i_bone] <= 0) return 1;
				SetPVarInt(playerid , "Inv: Item" , p_info[playerid][inv][slot]);
				new query_mysql[160];
				mysql_format(sql_connection, query_mysql, sizeof(query_mysql), "SELECT * FROM `attachplayer` WHERE usid = %d AND modelid = %d", p_info[playerid][id], inv_info[p_info[playerid][inv][slot]][i_object]);
				mysql_tquery(sql_connection, query_mysql, "load_user_acces", "id", playerid , i);
				return 1;
			}
		}

		for(new td = 1; td < 31; td++)
		{
			if(playertextid != td_inventory_p[playerid][td]) continue;

			new i = td - 1;
			if(p_info[playerid][inv][i] > 0) // если в слоте, что-то есть
			{
				if(GetPVarInt(playerid, !"Inv: Select") <= -1) // если нету активных слотов, то активируем этот слот
				{
					SetPVarInt(playerid, !"Inv: Select" , i);
					PlayerTextDrawBackgroundColor(playerid, td_inventory_p[playerid][td], 0xFFFFE0FF);
					PlayerTextDrawShow(playerid, td_inventory_p[playerid][td]);
					return 1;
				}
				else if(GetPVarInt(playerid, !"Inv: Select") == i) // если слот тот же, что и активированный
				{
					PlayerTextDrawBackgroundColor(playerid, td_inventory_p[playerid][td], 1061109759);
					SetPVarInt(playerid, !"Inv: Select" , -1);
					PlayerTextDrawShow(playerid, td_inventory_p[playerid][td]);
					return 1;
				}
			}
			else if(p_info[playerid][inv][i] == 0) // если слот пустой
			{
				if(GetPVarInt(playerid, !"Inv: Select") >= 0 && GetPVarInt(playerid, !"Inv: Select") < 30) // если есть активный слот, то перемещаем предмет
				{
					new i_s = GetPVarInt(playerid, !"Inv: Select");
					p_info[playerid][inv][i] = p_info[playerid][inv][i_s];
					p_info[playerid][inv_count][i] = p_info[playerid][inv_count][i_s];

					p_info[playerid][inv][i_s] = 0;
					p_info[playerid][inv_count][i_s] = 0;

					PlayerTextDrawFont(playerid, td_inventory_p[playerid][i_s + 1], 4);
					PlayerTextDrawColor(playerid, td_inventory_p[playerid][i_s + 1], 1061109759);

					new item = p_info[playerid][inv][i];
					PlayerTextDrawFont(playerid, td_inventory_p[playerid][td], 5);
					PlayerTextDrawSetPreviewModel(playerid, td_inventory_p[playerid][td], inv_info[item][i_object]);
					PlayerTextDrawSetPreviewRot(playerid, td_inventory_p[playerid][td], inv_info[item][i_cor][0], inv_info[item][i_cor][1], inv_info[item][i_cor][2], inv_info[item][i_cor][3]);
					PlayerTextDrawColor(playerid, td_inventory_p[playerid][td], 0xFFFFFFFF);
					PlayerTextDrawBackgroundColor(playerid, td_inventory_p[playerid][td], 1061109759);

					PlayerTextDrawShow(playerid, td_inventory_p[playerid][i_s + 1]);
					PlayerTextDrawShow(playerid, td_inventory_p[playerid][td]);

					return SetPVarInt(playerid, !"Inv: Select", -1);
				}
				else if(GetPVarInt(playerid, !"Inv: Select") >= 30)
				{
					new slot = GetPVarInt(playerid, !"Inv: Select") - 30, sec = player_veh_area[playerid], item = veh_info[sec][v_item][slot];
					if(!give_item(playerid, veh_info[sec][v_item][slot], veh_info[sec][v_item_count][slot])) return 0;

					veh_info[sec][v_item][slot] = 0;
					veh_info[sec][v_item_count][slot] = 0;

					PlayerTextDrawFont(playerid, td_inventory_p[playerid][td], 5);
