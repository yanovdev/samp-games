cmd:paygang(playerid, params[])
{
	if(!gang_player(playerid)|| !p_info[playerid][leader]) return 1;
	extract params -> new price_paygang; else return SendClientMessage(playerid, col_gray, !"Используйте: "c_white"/paygang [сумма]");
	if(price_paygang < 1 || price_paygang > 100000) return SendClientMessage(playerid, col_white, !""scm_error"Используйте сумму в диапазоне от 1$ до 100.000$");

	new num = 0;
	foreach(new i: logged_players)
	{
		if(p_info[i][member] != p_info[playerid][member] || playerid == i) continue;
		num++;
	}
	if(num <= 0) return SendClientMessage(playerid, col_white, !""scm_error"На данный момент нет игроков, которым можно выдать заработную плату.");

	SetPVarInt(playerid, !"gangpay_sum", price_paygang);

	new dialog_string[160];
	format(dialog_string, sizeof(dialog_string), ""c_white"Вы действительно хотите выдать %d$, игрокам состоящим в Вашей банде?\n"c_grey"Сумма может измениться при изменении онлайна организации.", price_paygang * num);
	return show_dialog(playerid, d_paygang, DIALOG_STYLE_MSGBOX, !""scm_dialog"Заработная плата", dialog_string, !"Далее", !"Отмена");
}

CMD:pay(playerid, params[])
{
	if(duel_opponent[playerid] != INVALID_PLAYER_ID) return 1;

 	if(sscanf(params, "dd", params[0], params[1])) return SendClientMessage(playerid, col_gray,"Используйте: /pay[ид][сумма]");
	if(! IsPlayerConnected(params[0])|| params[0] == playerid) return SendClientMessage(playerid, col_gray,"Игрок не найден.");
    if(! IsPlayerInRangeOfPoint(playerid, 5, p_t_info[params[0]][p_pos][0], p_t_info[params[0]][p_pos][1], p_t_info[params[0]][p_pos][2])|| GetPlayerVirtualWorld(params[0]) != GetPlayerVirtualWorld(playerid)) return SendClientMessage(playerid, col_gray,"Игрок слишком далеко.");


	if(p_info[params[0]][level] < 2 && params[1] > 500) return SendClientMessage(playerid, col_gray,"Вы не можете передать деньги игроку 1-го уровня больше 500$");
	if(params[1] < 1 || params[1] > 10000) return SendClientMessage(playerid, col_gray,"Можно передавать от 1 до 10000$");
	if(params[1] > p_info[playerid][money]) return SendClientMessage(playerid, col_gray,"Недостаточно денег для передачи.");

	if(GetString(p_t_info[params[0]][p_ip], p_t_info[playerid][p_ip]))
	{
		new scm_string[128];
		format(scm_string, sizeof(scm_string), ""c_red"A.WARNING: "c_grey"%s[%d]передаёт средства %s[%d]| same ip", p_info[playerid][name], playerid, p_info[params[0]][name], params[0]);
		foreach(new i: admin_players)SendClientMessage(i, col_gray, scm_string);

		SendClientMessage(playerid, col_gray,"Невозможно передать средства данному игроку.");
		return 1;
	}


	give_money(playerid, - params[1]);
	give_money(params[0], params[1]);

	new scm_string[128];
	format(scm_string, sizeof(scm_string), "%s передал вам "c_green"%d$"c_white".", p_info[playerid][name], params[1]);

	SendClientMessage(params[0], col_white, scm_string);

	format(scm_string, sizeof(scm_string), "Вы передали игроку %s "c_green"%d$"c_white".", p_info[params[0]][name], params[1]);
	SendClientMessage(playerid, col_white, scm_string);

	format(scm_string, sizeof(scm_string), "достал деньги и передал их %s", p_info[params[0]][name]);
	me_action(playerid, scm_string);

	insert_money_log(params[0], playerid, params[1], "команда /pay");
	return true;
}

CMD:createarea(playerid, params[])
{
	if(p_info[playerid][admin] < 8) return SendClientMessage(playerid, col_gray,"Данная команда не существует.");
	GetPlayerFacingAngle(playerid, p_t_info[playerid][p_pos][3]);
	for(new i = 0; i < 4; i ++)
	{
		new pvar_string[28];
