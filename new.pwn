	SendClientMessage(rename_id, col_gray, sql_string);
	SendClientMessage(rename_id, col_gray, ""c_grey"Рекомендуем вам перезайти в игру с новым никнеймом.");
	format(sql_string, 132, ""c_server"A.INFO: "c_grey"%s сменил никнейм на %s | Одобрил: %s", p_info[rename_id][name], change_name[rename_id], p_info[playerid][name]);
