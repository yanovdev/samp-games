	}

	AddVehicleComponent(veh_id , tuning_detal[playerid][0][0]);

	SetPVarInt(playerid , "Tuning:Max" , array);
	return 1;
}

stock get_ammount_veh(veh)
{
	new car = GetVehicleModel(veh);
	switch(car)
	{
		case 413 , 414 , 427 , 428 , 440 , 459 , 482 , 498 , 499 , 573 , 609 : return 1500;
		case 433 , 455 , 456 , 548 : return 2000;
	}
	return 1000;
}

stock Float: get_veh_ammount(veh)
{
	new Float: count;
	for(new s = 0; s < 15; s++)
	{
		if(veh_info[veh][v_item][s] <= 0)continue;
		count = inv_info[veh_info[veh][v_item][s]][i_count]* veh_info[veh][v_item_count][s];
	}
	return count;
}

stock update_veh_ammount(playerid , veh)
{
	new src[50];
	format(src , sizeof(src), "__vehicle____count:_%.1f_/_%d_kg" , get_veh_ammount(veh), get_ammount_veh(veh));
	PlayerTextDrawSetString(playerid , td_inventory_p[playerid][40], src);

	return 1;
}

stock give_veh_item(veh, item, ammount = 1)
{
	if((get_veh_ammount(veh) + (inv_info[item][i_count] * ammount)) > get_ammount_veh(veh + 1)) return 0;

	new slot = -1, maxs = 0;
	for(new s = 0; s < 15; s++)
	{
		if(veh_info[veh][v_item][s] == 0)
		{
			slot = s;
			break;
		}
		maxs ++;
	}
	if(slot != -1)
	{
		veh_info[veh][v_item][slot] = item;
		veh_info[veh][v_item_count][slot] = ammount;
		return true;
	}
	if(maxs > 0) return 0;
	return 0;
}

stock take_veh_item(veh, item, ammount = 1)
{
	for(new s = 0; s < 15; s++)
	{
		if(veh_info[veh][v_item][s] != item) continue;
		if(veh_info[veh][v_item_count][s] > ammount) return 0;

		veh_info[veh][v_item_count][s] -= ammount;
		if(veh_info[veh][v_item_count][s] <= 0) veh_info[veh][v_item][s] = 0;
		return 1;
	}
	return 0;
}

stock take_item(playerid, item, ammount = 1, slot = -1)
{
	if(slot >= 0)
	{
		if(p_info[playerid][inv][slot] != item || p_info[playerid][inv_count][slot] < ammount) return 0;
	}
	else
	{
		for(new i = 0; i < 30; i ++)
		{
			if(p_info[playerid][inv][i] != item) continue;
			if(p_info[playerid][inv_count][i] < ammount) return 0;
			slot = i;
			break;
		}
	}
	p_info[playerid][inv_count][slot] -= ammount;

	if(p_info[playerid][inv_count][slot] <= 0)
		p_info[playerid][inv][slot] = 0;

	if(inv_active[playerid] == 1)
		show_player_inventory(playerid, 1);

	return true;
}

stock give_item(playerid, item, ammount = 1, value = 1, todo = -1)
{
	new error, i_yes = -1, i_null = -1;
	if(get_value_ammount(playerid, item, ammount)) error = 1;
	if(!get_slot_null(playerid)) error = 2;

	if(error)
	{
		switch(value)
		{
			case 1: // от сервера
			{
				if(error == 1) SendClientMessage(playerid, col_white, !""scm_error"У Вас слишком много тяжелых вещей.");
				else if(error == 2) SendClientMessage(playerid, col_white, !""scm_error"У Вас нету свободного места в инвентаре.");
			}
			case 2: // от админа
			{
				if(error == 1) SendClientMessage(todo, col_white, !""scm_error"У игрока слишком большая масса инвентаря.");
				else if(error == 2) SendClientMessage(todo, col_white, !""scm_error"У игрока нет свободного места в инвентаре.");
			}
		}
		return 0;
	}

	for(new i = 0; i < 30; i ++)
	{
		if(p_info[playerid][inv][i] == 0 && i_null == -1) i_null = i;
		if(p_info[playerid][inv][i] == item)
		{
			i_yes = i;
			break;
		}
	}

	if(i_yes >= 0)
	{
		p_info[playerid][inv_count][i_yes] += ammount;
	}
	else if(i_null >= 0)
	{
		p_info[playerid][inv][i_null] = item;
		p_info[playerid][inv_count][i_null] = ammount;
	}

	switch(value)
	{
		case 2: // от админа
		{
			new text_string[144];
			format(text_string, sizeof(text_string), ""scm_info"Администратор "c_server"%s"c_white" выдал Вам предмет %s [%d]. Используйте команду: "c_gold"/inv", p_info[todo][name], inv_info[item][i_name], value);
			SendClientMessage(playerid, col_white, text_string);

			format(text_string, sizeof(text_string), ""scm_info"Вы успешно выдали игроку "c_server"%s"c_white" предмет %s [%d]", p_info[playerid][name], inv_info[item][i_name], value);
			SendClientMessage(todo, col_white, text_string);
		}
	}
	if(inv_active[playerid]) show_player_inventory(playerid, 1);
	return 1;
}

stock set_item_value(playerid , item , value = 1)
{
	for(new i = 0; i < 30; i ++)
	{
		if(p_info[playerid][inv][i] != item)continue;
		if(p_info[playerid][inv_count][i] < value) return false;
		if(p_info[playerid][inv_count][i] == value)p_info[playerid][inv][i] = 0;
		p_info[playerid][inv_count][i] -= value;
		return true;
	}
	return 0;
}

stock get_item_to_inv(playerid , item , value = 1)
{
	for(new i = 0; i < 30; i ++)
	{
		if(p_info[playerid][inv][i] != item)continue;
		if(p_info[playerid][inv_count][i] < value) return false;
		return true;
 	}
 	return false;
}

stock get_slot_null(playerid)
{
	for(new i = 0; i < 30; i++)
	{
		if(p_info[playerid][inv][i] > 0)continue;
		return true;
	}
	return false;
}

stock get_value_ammount(playerid , item , ammount , Float: value = 0.0)
{
	new Float: count;
	for(new i = 0; i < 30; i++)
	{
		if(p_info[playerid][inv][i] <= 0)continue;
		if(p_info[playerid][inv][i] >= 123 && p_info[playerid][inv][i] <= 131)value += inv_info[p_info[playerid][inv][i]][i_count]+(inv_info[135][i_count]* p_info[playerid][inv_count][i]); // подсчет патрон в оружие
		else value += inv_info[p_info[playerid][inv][i]][i_count]* p_info[playerid][inv_count][i];
	}
	if(item >= 123 && item <= 131)count = inv_info[item][i_count]+(inv_info[135][i_count]* ammount);
	else count = inv_info[item][i_count]* ammount;
	if((value + count) > p_info[playerid][inv_value_max]) return 1;
	return 0;
}

stock show_add_section(playerid , type)// 0 - мусорка	1 - машина
{
	new max_td = 15, slot;
	TextDrawTextSize(td_inventory[6], 185.5502, 238.000);

	switch(type)
	{
		case 0:
		{
			if(player_garb_area[playerid] < 0) return SendClientMessage(playerid, col_white, !""scm_error"Внутренняя ошибка # I - 001. Сделайте скриншот и обратитесь к администрации");
			PlayerTextDrawSetString(playerid , td_inventory_p[playerid][40], "__garbage");
			slot = player_garb_area[playerid];
			if(garb_info[player_garb_area[playerid]][g_type])max_td = 10;
		}
		case 1:
		{
			if(player_veh_area[playerid] < 0) return SendClientMessage(playerid, col_white, !""scm_error"Внутренняя ошибка # I - 003. Сделайте скриншот и обратитесь к администрации");
			slot = player_veh_area[playerid];
		}
	}

	new item, Float: count;
	for(new g = 0; g < 15; g ++)
	{
		new td = g + 41;

		if(type == 0)item = garb_info[slot][g_item][g];
		else if(type == 1)item = veh_info[slot][v_item][g];

		if(item > 0)
		{
			PlayerTextDrawFont(playerid, td_inventory_p[playerid][td], 5);
			PlayerTextDrawSetPreviewModel(playerid, td_inventory_p[playerid][td], inv_info[item][i_object]);
			PlayerTextDrawSetPreviewRot(playerid, td_inventory_p[playerid][td], inv_info[item][i_cor][0], inv_info[item][i_cor][1], inv_info[item][i_cor][2], inv_info[item][i_cor][3]);
			PlayerTextDrawColor(playerid, td_inventory_p[playerid][td], 0xFFFFFFFF);
			PlayerTextDrawBackgroundColor(playerid, td_inventory_p[playerid][td], 1061109759);
			if(type == 1)count += inv_info[item][i_count];
		}
		else
		{
			PlayerTextDrawFont(playerid , td_inventory_p[playerid][td], 4);
			PlayerTextDrawColor(playerid , td_inventory_p[playerid][td], 1061109759);
		}
	}
	if(type == 1)
	{
		new src[50];
		format(src , sizeof(src), "__vehicle____count:_%.1f_/_%d_kg" , count , get_ammount_veh(slot - 1));
		PlayerTextDrawSetString(playerid , td_inventory_p[playerid][40], src);
	}
	TextDrawShowForPlayer(playerid , td_inventory[6]);

	for(new td = 40; td < max_td + 41; td ++)PlayerTextDrawShow(playerid , td_inventory_p[playerid][td]);
	add_section[playerid] = 1;
	return 1;
}

