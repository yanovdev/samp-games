	format(sql_string, 132, "<< %s сменил имя на %s >>", p_info[rename_id][name], change_name[rename_id]);
	foreach(new i: logged_players)if(p_info[i][settings][2])SendClientMessage(i, col_light_red, sql_string);


	format(sql_string, sizeof(sql_string), "INSERT INTO `nickname_history`(`nh_owner`,`nh_oldname`,`nh_newname`,`nh_date`,`nh_admin`)VALUES('%d','%s','%s',NOW(),'%s')",
