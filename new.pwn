callback: mc_callback(playerid, to_id)
{
	new rows;
	cache_get_row_count(rows);
	if(! rows) return SendClientMessage(playerid, col_gray,"У вас нет медицинской карты.");
	new cmd_mc_date,
		cmd_mc_result;
	cache_get_value_name_int(0, "mc_result", cmd_mc_result);
	cache_get_value_name_int(0, "mc_date", cmd_mc_date);

	if((cmd_mc_date + 60*60*24*10) < gettime())
	{
		show_dialog(playerid, d_none, DIALOG_STYLE_MSGBOX, ""c_server"Медицинская карта", "{FFFFFF}Срок годности вашей мдцинской карты истек.", "Закрыть", "");
		new scm_string[98];
		format(scm_string, sizeof(scm_string ), "DELETE FROM `users_medcards` WHERE `mc_id` = %d", p_info[playerid][id]);
		mysql_tquery(sql_connection, scm_string, "", "", playerid);
		return show_dialog(playerid, d_none, DIALOG_STYLE_MSGBOX, ""c_server"Медицинская карта", "{FFFFFF}Срок годности вашей мдцинской карты истек.", "Закрыть", "");
	}

	static const mc_status[][] = {
		"Серьезно болен/больна",
		"Наблюдаются отклонения",
		"Полностью здоров(а)" };

	if(to_id == playerid)format(global_string, 72, "посмотрел(-а)свою мед.карту");
	else format(global_string, 72, "показал(-а)мед.карту %s", p_info[to_id][name]);

	me_action(playerid, global_string);

	new dialog_string[186];

	format(dialog_string, sizeof(dialog_string), "{ffffff}Владелец: %s\nДата выдачи: %s\nДействительна до: %s\n\n\n{cccc99}%s",
	p_info[playerid][name], date("%dd.%mm.%yyyy", cmd_mc_date), date("%dd.%mm.%yyyy", cmd_mc_date + 60*60*24*10), mc_status[cmd_mc_result]);

	show_dialog(to_id, d_none, DIALOG_STYLE_MSGBOX, ""c_server"Медицинская карта", dialog_string, "Закрыть", "");
	return 1;
}


