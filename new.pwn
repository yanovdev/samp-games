					PlayerTextDrawShow(playerid, td_inventory_p[playerid][td]);

					td = slot + 41;

					PlayerTextDrawFont(playerid, td_inventory_p[playerid][td], 4);
					PlayerTextDrawColor(playerid, td_inventory_p[playerid][td], 1061109759);
					PlayerTextDrawShow(playerid, td_inventory_p[playerid][td]);

					SetPVarInt(playerid, !"Inv: Select", -1);
					update_veh_ammount(playerid, sec);
